clear      lock 
clear   control
clear      mod1
clear      mod2
clear      mod3
clear      mod4
clear      mod5
keycode      66 = Alt_R
add     control = Control_L Control_R
add        mod1 = Alt_L Meta_L
add        mod2 = Num_Lock
add        mod3 = Alt_R
add        mod4 = Super_L Super_R
add        mod5 = Mode_switch ISO_Level3_Shift  